<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MobileFeaturesTest.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MobileFeaturesTest"
    Title="MobileFeaturesTest">
    <TabBar>
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
        <ShellContent
            Title="GPS"
            ContentTemplate="{DataTemplate local:GPS}"
            Route="GPS" />
    </TabBar>
    

</Shell>
